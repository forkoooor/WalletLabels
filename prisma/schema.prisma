generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id 
  address   String  @unique

  name      String
  email     String?
  avatar    String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
