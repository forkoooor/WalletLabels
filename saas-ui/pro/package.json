{
  "name": "@saas-ui/pro",
  "version": "0.5.3",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./theme": {
      "require": "./dist/theme/index.ts",
      "default": "./dist/theme/index.mjs"
    },
    "./src": {
      "default": "./src/index.ts"
    },
    "./src/theme": {
      "default": "./src/theme/index.ts"
    }
  },
  "publishConfig": {
    "registry": "https://npm.saas-ui.dev"
  },
  "scripts": {
    "bundle": "yarn pack --out ../dist/%s-%v.tgz",
    "build": "tsup src/index.ts src/theme/index.ts --config tsup.config.ts",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "src",
    "index.ts",
    "theme.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/saas-js/saas-ui-pro.git",
    "directory": "saas-ui/pro"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.0.1",
    "@chakra-ui/clickable": "^2.0.4",
    "@chakra-ui/react-utils": "^2.0.1",
    "@chakra-ui/system": "^2.2.2",
    "@chakra-ui/theme-tools": "^2.0.5",
    "@chakra-ui/utils": "^2.0.4",
    "@react-hookz/web": "^15.0.0",
    "@saas-ui/onboarding": "workspace:*",
    "@saas-ui/react-utils": "^1.0.0",
    "@saas-ui/router": "workspace:*",
    "@saas-ui/theme": "^1.1.0",
    "@tanstack/react-table": "^8.2.6",
    "date-fns": "^2.28.0",
    "polished": "^4.2.2"
  },
  "peerDependencies": {
    "@chakra-ui/react": ">=2",
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "@saas-ui/react": ">=1",
    "framer-motion": ">=5.5.0",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "react-icons": "^4.4.0",
    "tsup": "^6.1.3"
  }
}
