{
  "name": "@saas-ui/pro",
  "version": "0.17.0",
  "engines": {
    "node": ">=14 <=18"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./theme": {
      "require": "./dist/theme/index.js",
      "types": "./dist/theme/index.d.ts",
      "import": "./dist/theme/index.mjs"
    },
    "./glass-theme": {
      "require": "./dist/glass-theme/index.js",
      "types": "./dist/glass-theme/index.d.ts",
      "import": "./dist/glass-theme/index.mjs"
    },
    "./src": {
      "default": "./src/index.ts"
    },
    "./src/theme": {
      "default": "./src/theme/index.ts"
    },
    "./src/glass-theme": {
      "default": "./src/glass-theme/index.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.saas-ui.dev"
  },
  "scripts": {
    "bundle": "yarn pack --out ../dist/%s-%v.tgz",
    "build": "tsup src/index.ts src/theme/index.ts --config tsup.config.ts",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/saas-js/saas-ui-pro.git",
    "directory": "saas-ui/pro"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.1.2",
    "@chakra-ui/clickable": "^2.0.14",
    "@chakra-ui/react-utils": "^2.0.12",
    "@chakra-ui/styled-system": "^2.5.2",
    "@chakra-ui/system": "^2.4.0",
    "@chakra-ui/theme-tools": "^2.0.17",
    "@chakra-ui/utils": "^2.0.15",
    "@react-hookz/web": "^15.1.0",
    "@saas-ui/onboarding": "workspace:*",
    "@saas-ui/react-utils": "^1.2.0",
    "@saas-ui/router": "workspace:*",
    "@saas-ui/theme": "^1.8.0",
    "@tanstack/react-table": "^8.7.6",
    "date-fns": "^2.29.3",
    "polished": "^4.2.2"
  },
  "peerDependencies": {
    "@chakra-ui/react": ">=2.4.8",
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "@saas-ui/react": ">=1.8.0",
    "framer-motion": ">=5.5.0",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@ngneat/falso": "^2.27.0",
    "react-icons": "^4.7.1",
    "tsup": "^6.5.0"
  }
}
