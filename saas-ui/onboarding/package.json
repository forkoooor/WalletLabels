{
  "name": "@saas-ui/onboarding",
  "version": "0.5.0",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./src": {
      "default": "./src/index.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.saas-ui.dev"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/saas-js/saas-ui-pro.git",
    "directory": "saas-ui/onboarding"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "bundle": "yarn pack --out ./dist/%s-%v.tgz",
    "build": "tsup src/index.ts --config tsup.config.ts",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.0.1",
    "@chakra-ui/react-utils": "^2.0.1",
    "@chakra-ui/system": "^2.2.2",
    "@chakra-ui/theme-tools": "^2.0.5",
    "@chakra-ui/utils": "^2.0.4",
    "@saas-ui/react-utils": "^1.0.0"
  },
  "peerDependencies": {
    "@chakra-ui/react": ">=2.1",
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "@saas-ui/react": ">=1.2.0",
    "framer-motion": ">=5.5.0",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "tsup": "^6.1.3"
  }
}
